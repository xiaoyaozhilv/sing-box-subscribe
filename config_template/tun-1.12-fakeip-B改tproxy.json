		  "geosite-spotify",
		  "geosite-tiktok",
		  "geoip-netflix",
          "geosite-netflix",
		  "geosite-disney",
		  "geosite-category-games",
		  "geosite-hbo",
          "geosite-primevideo",
		  "geosite-geolocation-!cn"
        ],
        "outbound": "Proxy"
      },
      {
        "rule_set": [
          "geosite-bilibili",
		  "geoip-cn",
          "geosite-cn",
          "geosite-alibaba",
          "geosite-jd"		  
        ],
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geoip-apple",
          "geosite-apple",
          "geosite-amazon"
